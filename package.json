{
  "name": "licences",
  "version": "0.1.0",
  "description": "Licences Application",
  "main": "server.js",
  "engines": {
    "node": "^8.11.1",
    "npm": "^5.6.0"
  },
  "scripts": {
    "start": "node server.js",
    "test": "mocha",
    "test-with-coverage": "nyc --reporter=text --check-coverage --lines 85 mocha",
    "test-with-coverage-ci": "nyc --reporter=html --check-coverage --lines 85 mocha --reporter mocha-junit-reporter",
    "plant-beanstalk": "node bin/plant-beanstalk.js",
    "lint": "eslint .",
    "record-build-info": "node ./bin/record-build-info",
    "wait-for-deploy": "node ./bin/wait-for-deploy",
    "build": "npm run clean && npm run css-build",
    "dev": "npm run build && npm run lint && npm run test && npm run start-watchmode",
    "css-build": "./bin/build-css",
    "clean": "rm -rf public/* .port.tmp *.log build/* uploads/* test-results.xml",
    "start-watchmode": "nodemon --watch server server.js",
    "knex": "knex",
    "knex-migrate": "knex-migrate",
    "migrate": "knex migrate:latest --env standard",
    "seed": "knex seed:run --env standard",
    "demo": "knex seed:run --env demo"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,json,css,md}": [
        "prettier --write",
        "eslint --fix",
        "git add"
      ]
    },
    "ignore": [
      "package.json"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noms-digital-studio/licences.git"
  },
  "author": "HMPPS Digital Studio",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/noms-digital-studio/licences/issues"
  },
  "homepage": "https://github.com/noms-digital-studio/licences#readme",
  "dependencies": {
    "applicationinsights": "^1.1.0",
    "body-parser": "^1.18.3",
    "case": "^1.6.1",
    "compression": "^1.7.3",
    "config": "^1.31.0",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.4",
    "cookie-session": "^2.0.0-beta.3",
    "csurf": "^1.9.0",
    "dotenv": "^6.2.0",
    "eslint-config-google": "^0.9.1",
    "express": "^4.16.4",
    "express-request-id": "^1.4.1",
    "express-winston": "^2.6.0",
    "govuk-elements-sass": "^3.1.3",
    "govuk_frontend_toolkit": "^5.2.0",
    "govuk_template_jinja": "^0.24.1",
    "helmet": "^3.15.1",
    "joi": "^13.7.0",
    "joi-date-extensions": "^1.2.0",
    "joi-postcode": "^3.0.0",
    "jquery": "^1.12.4",
    "jsonwebtoken": "^8.4.0",
    "knex": "^0.15.2",
    "knex-migrate": "^1.7.0",
    "moment": "^2.24.0",
    "node-sass": "^4.11.0",
    "node-sass-middleware": "^0.11.0",
    "nodemon": "^1.18.10",
    "notifications-node-client": "^4.6.0",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "path": "^0.12.7",
    "pg": "^7.8.0",
    "poll-endpoint-for": "0.0.4",
    "pug": "^2.0.3",
    "querystring": "^0.2.0",
    "ramda": "^0.25.0",
    "superagent": "^3.8.3",
    "swagger-ui-express": "^4.0.2",
    "util": "^0.11.1",
    "uuid": "^3.3.2",
    "winston": "^2.4.4",
    "winston-azure-application-insights": "^2.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "dirty-chai": "^2.0.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-prettier": "^3.0.1",
    "husky": "^1.3.1",
    "istanbul": "^0.4.5",
    "lint-staged": "^8.1.4",
    "mocha": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "nock": "^9.6.1",
    "nyc": "^13.3.0",
    "prettier": "1.16.4",
    "proxyquire": "^2.1.0",
    "sinon": "^6.3.5",
    "sinon-chai": "^3.3.0",
    "supertest": "^3.4.2"
  }
}
