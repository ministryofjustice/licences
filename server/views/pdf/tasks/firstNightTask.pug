
- var incomplete = incompleteGroups && incompleteGroups.includes('firstNight')
- var buttonState = incomplete ? 'UNSTARTED' : 'DONE'

div.paddingBottom
    h2.heading-medium First Night Curfew Hours

    div.pure-g
        div.pure-u-1.pure-u-md-3-4#firstNightTaskStatus
            if incomplete
                | Not complete
            else
                | Complete
        div.pure-u-1.pure-u-md-1-4.center
            +startContinueChange(buttonState, "/hdc/curfew/firstNight/" + templateName + "/" + prisoner.bookingId)
