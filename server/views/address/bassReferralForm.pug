block bassReferralForm

    - var vals = bassReferral ? bassReferral : '';

    div.pure-g
        div.pure-u-1-3
            div.paddingBottom
                label(for='proposedTown') Proposed Town:
                input.form-control(type='text' name='proposedTown' id='proposedTown' value=vals.proposedTown ? vals.proposedTown : '')
            div
                label(for='proposedCounty') Proposed County:
                input.form-control(type='text' name='proposedCounty' id='proposedCounty' value=vals.proposedCounty ? vals.proposedCounty : '')
