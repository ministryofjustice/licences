module.exports = function(audit) {

    function getSend(transitionType) {
        return (bookingId) => {
            if (bookingId) {
                return audit.getEvents('SEND', {bookingId, transitionType});
            }
            return audit.getEvents('SEND', {transitionType});
        }
    }

    return {
        getAddressSubmission: getSend('caToRo'),
        getAssessmentComplete: getSend('roToCa')
    }
};
