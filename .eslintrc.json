{
  "extends": ["airbnb-base", "plugin:prettier/recommended"],
  "rules": {
    "no-unused-vars": [
      1,
      {
        "argsIgnorePattern": "res|next|^err|_",
        "ignoreRestSiblings": true
      }
    ],
    "prettier/prettier": ["error"],
    "no-use-before-define": "off",
    "prefer-destructuring": "warn",
    "consistent-return": "off",
    "no-console": ["error"]
  },
  "plugins": ["prettier"]
}
